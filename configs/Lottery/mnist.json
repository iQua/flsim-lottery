{
    "clients": {
        "total": 1000,
        "per_round": 20,
        "label_distribution": "uniform",
        "do_test": false,
        "test_partition": 0.2
    },
    "data": {
        "loading": "dynamic",
        "partition": {
            "size": 600,
            "range": [
                50,
                200
            ]
        },
        "IID": false,
        "bias": {
            "primary": 0.8,
            "secondary": false
        }
    },
    "federated_learning": {
        "rounds": 200,
        "target_accuracy": 0.95,
        "task": "train",
        "epochs": 5,
        "batch_size": 10
    },
    "paths": {
        "data": "./data",
        "model": "./models",
        "reports": "reports.pkl"
    },
    "server": "lth",
    "lottery": {
        "subcommand": "lottery",
        "platform": "local",
        "display_output_location": false,
        "num_workers": 0,
        "replicate": 1,
        "default_hparams": null,
        "quiet": false,
        "evaluate_only_at_end": false,
        "levels": 12,
        "rewinding_steps": null,
        "pretrain": false,
        "dataset_name": "cifar10",
        "batch_size": 64,
        "do_not_augment": false,
        "transformation_seed": null,
        "subsample_fraction": null,
        "random_labels_fraction": null,
        "unsupervised_labels": null,
        "blur_factor": null,
        "model_name": "cifar_vgg_19",
        "model_init": "kaiming_normal",
        "batchnorm_init": "uniform",
        "batchnorm_frozen": false,
        "output_frozen": false,
        "others_frozen": false,
        "others_frozen_exceptions": null,
        "optimizer_name": "sgd",
        "lr": 0.1,
        "training_steps": "80ep",
        "data_order_seed": null,
        "momentum": 0.9,
        "nesterov_momentum": 0.0,
        "milestone_steps": "80ep,120ep",
        "gamma": 10.0,
        "warmup_steps": "10000it",
        "weight_decay": null,
        "apex_fp16": false,
        "pruning_strategy": "sparse_global",
        "pruning_fraction": 0.2,
        "pruning_layers_to_ignore": null
    }
